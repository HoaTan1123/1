Rayfield:Notify({Title = "Anti-Cheating Bypasså·²å°±ç»ª", Content = "å¼€å¯åŽäº«å—é«˜çº§é˜²æŠ¤", Duration = 6})

task.spawn(function()
    while task.wait(5) do
        if #statusLabels > 0 then
            pcall(function()
                for _, label in pairs(statusLabels) do
                    label:Destroy()
                end
                statusLabels = {}
            end)
        end
    end
end)
