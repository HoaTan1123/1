local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "Anti-Cheating Bypass",
   LoadingTitle = "é˜²æŠ¤ç³»ç»ŸåŠ è½½ä¸­",
   LoadingSubtitle = "æ­£åœ¨åˆå§‹åŒ–ä¿æŠ¤å±‚",
   ConfigurationSaving = {Enabled = true, FolderName = "AntiCheatBypass"},
   KeySystem = false
})

local MasterEnabled = false
local backupHooks = {}
local activeConnections = {}
local fakeRemotes = {}
local behaviorPattern = "Normal"
local startTime = tick()

local function SetupHookProtection()
    local mt = getrawmetatable(game)
    setreadonly(mt, false)
    local oldNamecall = mt.__namecall
    mt.__namecall = newcclosure(function(Self, ...)
        local method = getnamecallmethod()
        if method == "Kick" and Self == game.Players.LocalPlayer then
            for i = 1, 10 do
                game.StarterGui:SetCore("SendNotification", {
                    Title = "Anti-Cheatæ‹¦æˆªæˆåŠŸï¼",
                    Text = "æ£€æµ‹åˆ°è¸¢äººå°è¯•ï¼Œå·²ç»•è¿‡æ£€æµ‹ç³»ç»Ÿ",
                    Duration = 3
                })
                task.wait(0.3)
            end
            game.StarterGui:SetCore("SendNotification", {Title = "æ‹¦æˆªæˆåŠŸ", Duration = 1})
            return
        end
        return oldNamecall(Self, ...)
    end)
    setreadonly(mt, true)
    backupHooks.namecall = oldNamecall
    
    local originalKick = game.Players.LocalPlayer.Kick
    game.Players.LocalPlayer.Kick = function(self, ...)
        Rayfield:Notify({Title = "å¤‡ç”¨æ‹¦æˆªè§¦å‘", Content = "ç›´æŽ¥Kickè°ƒç”¨è¢«é˜»æ­¢ï¼Œå·²ç»•è¿‡", Duration = 3})
        return nil
    end
    backupHooks.kick = originalKick
end

local function SetupNetworkCamouflage()
    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    
    for i = 1, 3 do
        local fakeRemote = Instance.new("RemoteEvent")
        fakeRemote.Name = "ClientSignal_" .. math.random(1000,9999) .. "_" .. i
        fakeRemote.Parent = ReplicatedStorage
        table.insert(fakeRemotes, fakeRemote)
    end
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function()
        if not MasterEnabled then return end
        
        for _, remote in pairs(fakeRemotes) do
            pcall(function()
                remote:FireServer({
                    Type = "PlayerUpdate",
                    Position = game.Players.LocalPlayer.Character and 
                              game.Players.LocalPlayer.Character.HumanoidRootPart and
                              game.Players.LocalPlayer.Character.HumanoidRootPart.Position or Vector3.new(),
                    Health = 100,
                    Timestamp = os.time(),
                    IsLegit = true
                })
            end)
        end
    end)
    
    table.insert(activeConnections, connection)
end

local function SetupCharacterProtection()
    local Players = game.Players
    local LocalPlayer = Players.LocalPlayer
    
    local charRemoving = LocalPlayer.CharacterRemoving:Connect(function()
        task.wait(0.1)
        LocalPlayer:LoadCharacter()
        Rayfield:Notify({Title = "è§’è‰²é‡ç”Ÿ", Content = "è‡ªåŠ¨é‡è½½è§’è‰²å®Œæˆï¼Œå·²ç»•è¿‡å¼ºåˆ¶ç§»é™¤", Duration = 2})
    end)
    table.insert(activeConnections, charRemoving)
    
    local charAdded = LocalPlayer.CharacterAdded:Connect(function(char)
        task.wait(0.5)
        char:SetAttribute("AntiKill_Protected", true)
        
        local protectionScript = Instance.new("Script", char)
        protectionScript.Name = "AntiKill_" .. math.random(1000,9999)
        protectionScript.Enabled = true
        protectionScript.Source = [[
            local char = script.Parent
            while task.wait(1) do
                if not char:GetAttribute("AntiKill_Protected") then
                    char:SetAttribute("AntiKill_Protected", true)
                end
            end
        ]]
    end)
    table.insert(activeConnections, charAdded)
    
    local microMove = game:GetService("RunService").Stepped:Connect(function()
        if not MasterEnabled then return end
        
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            local root = char.HumanoidRootPart
            local offset = Vector3.new(
                math.random(-3,3)/200,
                math.random(-1,1)/200,
                math.random(-3,3)/200
            )
            root.CFrame = root.CFrame + offset
        end
    end)
    table.insert(activeConnections, microMove)
end

local function SetupRemoteMonitoring()
    local function hookRemote(remote)
        if remote:IsA("RemoteEvent") then
            local oldFire = remote.FireServer
            remote.FireServer = function(self, ...)
                local args = {...}
                for _, arg in pairs(args) do
                    if type(arg) == "string" and (arg:lower():find("kick") or arg:lower():find("ban")) then
                        Rayfield:Notify({Title = "è¿œç¨‹æ‹¦æˆª", Content = "æ£€æµ‹åˆ°è¸¢äººè¿œç¨‹è°ƒç”¨ï¼Œå·²é˜»æ­¢", Duration = 2})
                        return nil
                    end
                end
                return oldFire(self, ...)
            end
        elseif remote:IsA("RemoteFunction") then
            local oldInvoke = remote.InvokeServer
            remote.InvokeServer = function(self, ...)
                local args = {...}
                for _, arg in pairs(args) do
                    if type(arg) == "string" and arg:lower():find("kick") then
                        return {Success = false, Reason = "Blocked"}
                    end
                end
                return oldInvoke(self, ...)
            end
        end
    end
    
    for _, remote in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
        pcall(hookRemote, remote)
    end
    
    local connection = game:GetService("ReplicatedStorage").DescendantAdded:Connect(function(remote)
        task.wait(0.5)
        pcall(hookRemote, remote)
    end)
    table.insert(activeConnections, connection)
end

local function SetupBehaviorCamouflage()
    local behaviors = {"NormalMovement", "SlightLag", "RandomPauses", "HumanLikeMouse", "AFKPattern"}
    local lastSwitch = tick()
    local currentBehavior = "NormalMovement"
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function()
        if not MasterEnabled then return end
        
        if tick() - lastSwitch > math.random(10, 30) then
            currentBehavior = behaviors[math.random(1, #behaviors)]
            lastSwitch = tick()
            behaviorPattern = currentBehavior
        end
        
        if currentBehavior == "SlightLag" then
            task.wait(math.random(5, 15) / 1000)
        elseif currentBehavior == "RandomPauses" then
            if math.random(1, 100) < 10 then
                task.wait(0.1)
            end
        elseif currentBehavior == "AFKPattern" then
            if math.random(1, 100) < 30 then
                task.wait(math.random(1, 3))
            end
        end
    end)
    
    table.insert(activeConnections, connection)
end

local function SetupTeleportProtection()
    local teleportConnection = game.Players.LocalPlayer.OnTeleport:Connect(function(state)
        if state == Enum.TeleportState.Started then
            task.wait(0.1)
            game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
        end
    end)
    table.insert(activeConnections, teleportConnection)
end

local function SetupStatusMonitor()
    local lastUpdate = tick()
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function()
        if not MasterEnabled then return end
        
        if tick() - lastUpdate > 5 then
            lastUpdate = tick()
            
            local char = game.Players.LocalPlayer.Character
            if char then
                char:SetAttribute("LastCheck", os.time())
            end
            
            local runtime = math.floor(tick() - startTime)
            if runtime % 60 == 0 then
                Rayfield:Notify({
                    Title = "é˜²æŠ¤è¿è¡Œä¸­",
                    Content = string.format("å·²ä¿æŠ¤ %d åˆ†é’Ÿ", runtime // 60),
                    Duration = 1
                })
            end
        end
    end)
    
    table.insert(activeConnections, connection)
end

local function Cleanup()
    MasterEnabled = false
    
    for _, conn in pairs(activeConnections) do
        pcall(function() conn:Disconnect() end)
    end
    activeConnections = {}
    
    for _, remote in pairs(fakeRemotes) do
        pcall(function() remote:Destroy() end)
    end
    fakeRemotes = {}
    
    if backupHooks.namecall then
        local mt = getrawmetatable(game)
        setreadonly(mt, false)
        mt.__namecall = backupHooks.namecall
        setreadonly(mt, true)
    end
    
    if backupHooks.kick then
        game.Players.LocalPlayer.Kick = backupHooks.kick
    end
end

local Tab = Window:CreateTab("é«˜æ£€æµ‹ä¸“ç”¨", 4483362458)
local StatusTab = Window:CreateTab("çŠ¶æ€ç›‘æŽ§", 4483362458)
local ToolsTab = Window:CreateTab("å·¥å…·", 4483362458)

Tab:CreateToggle({
   Name = "ðŸ›¡ï¸ å¯åŠ¨åä½œå¼Šç»•è¿‡ç³»ç»Ÿ",
   CurrentValue = false,
   Callback = function(Value)
      if Value then
         MasterEnabled = true
         startTime = tick()
         
         SetupHookProtection()
         SetupNetworkCamouflage()
         SetupCharacterProtection()
         SetupRemoteMonitoring()
         SetupBehaviorCamouflage()
         SetupTeleportProtection()
         SetupStatusMonitor()
         
         for i = 1, 3 do
             Rayfield:Notify({Title = "Anti-Cheat Bypasså¯åŠ¨", Content = string.format("ç¬¬%då±‚é˜²æŠ¤å·²æ¿€æ´»", i), Duration = 2})
             task.wait(0.5)
         end
         
         Rayfield:Notify({Title = "åä½œå¼Šç»•è¿‡ç³»ç»Ÿå¯åŠ¨å®Œæˆ", Content = "æ£€æµ‹ç³»ç»Ÿå·²æˆåŠŸç»•è¿‡", Duration = 5})
      else
         Cleanup()
         Rayfield:Notify({Title = "é˜²æŠ¤å·²å…³é—­", Content = "æ‰€æœ‰é˜²æŠ¤å±‚å·²åœç”¨", Duration = 3})
      end
   end,
})

local statusLabels = {}
StatusTab:CreateButton({
   Name = "ðŸ”„ åˆ·æ–°çŠ¶æ€",
   Callback = function()
      for _, label in pairs(statusLabels) do
         label:Destroy()
      end
      statusLabels = {}
      
      table.insert(statusLabels, StatusTab:CreateLabel("=== é˜²æŠ¤ç³»ç»ŸçŠ¶æ€ ==="))
      table.insert(statusLabels, StatusTab:CreateLabel(string.format("è¿è¡Œæ—¶é—´: %dç§’", math.floor(tick() - startTime))))
      table.insert(statusLabels, StatusTab:CreateLabel(string.format("é˜²æŠ¤çŠ¶æ€: %s", MasterEnabled and "ðŸŸ¢ è¿è¡Œä¸­" or "ðŸ”´ å·²åœæ­¢")))
      table.insert(statusLabels, StatusTab:CreateLabel(string.format("è¡Œä¸ºæ¨¡å¼: %s", behaviorPattern)))
      table.insert(statusLabels, StatusTab:CreateLabel(string.format("æ´»è·ƒè¿žæŽ¥: %d", #activeConnections)))
      table.insert(statusLabels, StatusTab:CreateLabel(string.format("è™šå‡Remote: %d", #fakeRemotes)))
      table.insert(statusLabels, StatusTab:CreateLabel("=== é˜²æŠ¤å±‚çŠ¶æ€ ==="))
      table.insert(statusLabels, StatusTab:CreateLabel("1. Hookæ‹¦æˆª: ðŸŸ¢"))
      table.insert(statusLabels, StatusTab:CreateLabel("2. ç½‘ç»œä¼ªè£…: ðŸŸ¢"))
      table.insert(statusLabels, StatusTab:CreateLabel("3. è§’è‰²ä¿æŠ¤: ðŸŸ¢"))
      table.insert(statusLabels, StatusTab:CreateLabel("4. è¿œç¨‹ç›‘æŽ§: ðŸŸ¢"))
      table.insert(statusLabels, StatusTab:CreateLabel("5. è¡Œä¸ºä¼ªè£…: ðŸŸ¢"))
   end,
})

ToolsTab:CreateButton({
   Name = "ðŸš¨ ç´§æ€¥æ¢å¤è§’è‰²",
   Callback = function()
      local player = game.Players.LocalPlayer
      player:LoadCharacter()
      task.wait(0.5)
      if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
          player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 50, 0)
      end
      Rayfield:Notify({Title = "ç´§æ€¥æ¢å¤", Content = "è§’è‰²å·²å¼ºåˆ¶é‡ç”Ÿå¹¶é‡ç½®ä½ç½®", Duration = 3})
   end,
})

ToolsTab:CreateButton({
   Name = "ðŸ”§ ä¸€é”®å¢žå¼ºä¼ªè£…",
   Callback = function()
      for _, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
         if v:IsA("BasePart") then
            pcall(function() v:SetNetworkOwnershipAuto() end)
         end
      end
      
      for i = 1, 20 do
          game:GetService("RunService").Heartbeat:Wait()
          if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
              local root = game.Players.LocalPlayer.Character.HumanoidRootPart
              root.CFrame = root.CFrame + Vector3.new(math.random(-5,5)/50, 0, math.random(-5,5)/50)
          end
      end
      
      Rayfield:Notify({Title = "ä¼ªè£…å¢žå¼ºå®Œæˆ", Content = "ç½‘ç»œæ‰€æœ‰æƒé‡ç½®+æŠ–åŠ¨ä¼ªè£…æ¿€æ´»", Duration = 4})
   end,
})

ToolsTab:CreateButton({
   Name = "ðŸ§¹ æ¸…ç†ç—•è¿¹",
   Callback = function()
      Cleanup()
      task.wait(0.5)
      Rayfield:Notify({Title = "æ¸…ç†å®Œæˆ", Content = "æ‰€æœ‰é˜²æŠ¤ç—•è¿¹å·²æ¸…é™¤", Duration = 3})
   end,
})

Tab:CreateLabel("=== äº”é‡é˜²æŠ¤ç³»ç»Ÿ ===")
Tab:CreateLabel("1. Hookæ‹¦æˆªå±‚ - æ‹¦æˆªæ‰€æœ‰è¸¢äººè°ƒç”¨")
Tab:CreateLabel("2. ç½‘ç»œä¼ªè£…å±‚ - å‘é€è™šå‡ç½‘ç»œæ•°æ®")
Tab:CreateLabel("3. è§’è‰²ä¿æŠ¤å±‚ - è‡ªåŠ¨é‡ç”Ÿ+å¾®ç§»ä¼ªè£…")
